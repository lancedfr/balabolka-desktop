<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
       fx:controller="com.lance.balabolka.controller.Screen1Controller">
    <StackPane prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: #556699;">
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
            <Label fx:id="screenName" text="SCREEN1"/>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <children>
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text"/>
                    <TextField fx:id="textField"/>
                </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#handleScreen2ButtonAction" text="Screen2"/>
            <Button mnemonicParsing="false" onAction="#handleScreen3ButtonAction" text="Screen3"/>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </StackPane>
</Group>
